<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.lopes.heider.android_sensores" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.com.lopes.heider.android_sensores.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.com.lopes.heider.android_sensores.ListaSensores"
            android:label="@string/title_activity_lista_sensores" >
        </activity>
        <activity
            android:name="br.com.lopes.heider.android_sensores.SensorLuzActivity"
            android:label="@string/title_activity_sensor_luz" >
        </activity>
        <activity
            android:name="br.com.lopes.heider.android_sensores.SensorProximidadeActivity"
            android:label="@string/title_activity_sensor_proximidade" >
        </activity>
    </application>

</manifest>
